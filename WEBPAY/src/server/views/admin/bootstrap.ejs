<!doctype html>
<html>

<head>
    <meta charset="utf-8">
    <title><%= (branding && branding.displayName) || 'Admin' %> â€¢ Bootstrap</title>
    <link rel="stylesheet" href="/static/css/admin.css">
    <% if (brandCssStyle) { %>
        <%= brandCssStyle %>
            <% } %>
</head>

<body class="bg-gray-50">
    <%- include('./_brandbar', { branding }) %>
    <main
        style="max-width:720px;margin:3rem auto;padding:2rem;background:#fff;border-radius:1rem;box-shadow:0 1px 4px rgba(0,0,0,.08)">
        <h1 class="text-2xl font-semibold mb-4">Bootstrap</h1>
        <% if (unsignedCall) { %>
            <p>Unsigned admin bootstrap call:</p>
            <pre class="bg-gray-100 p-3 rounded"><%= JSON.stringify(unsignedCall, null, 2) %></pre>
            <% } else { %>
                <p class="text-gray-600">Use the bootstrap action to generate the unsigned call.</p>
                <% } %>
    </main>
    <%- hydrationScript || '' %>
</body>

</html>
