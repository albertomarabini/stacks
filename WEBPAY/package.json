{
    "name": "webpay",
    "private": true,
    "type": "commonjs",
    "scripts": {
        "dev": "concurrently -k -n srv,cli \"tsx watch src/server/index.ts\" \"node scripts/build-client.mjs --watch\"",
        "start": "node dist/src/server/index.js",
        "dev:debug": "concurrently -k -n srv,cli \"NODE_OPTIONS='--inspect=9229 --enable-source-maps' tsx watch src/server/index.ts\" \"node scripts/build-client.mjs --watch\"",
        "build": "rimraf dist && tsc -p tsconfig.json",
        "mapping": "node scripts/build-client.mjs --watch"
    },
    "engines": {
        "node": ">=18.18"
    },
    "dependencies": {
        "@stacks/connect": "8.2.0",
        "@stacks/network": "7.2.0",
        "@stacks/transactions": "7.2.0",
        "@stacks/wallet-sdk": "7.2.0",
        "better-sqlite3": "^12.4.1",
        "cookie-parser": "^1.4.7",
        "csurf": "^1.11.0",
        "dotenv": "^17.2.3",
        "ejs": "^3.1.10",
        "ejs-mate": "^4.0.0",
        "express": "^4.21.2",
        "express-session": "^1.18.2",
        "helmet": "^8.1.0",
        "qrcode": "^1.5.4",
        "uuid": "^13.0.0"
    },
    "devDependencies": {
        "@types/csurf": "^1.11.5",
        "@types/express": "^4.17.23",
        "@types/express-session": "^1.18.2",
        "@types/node": "^22.8.1",
        "@types/qrcode": "^1.5.5",
        "concurrently": "^9.2.1",
        "esbuild": "^0.23.1",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "tsx": "^4.20.6",
        "typescript": "^5.6.3"
    }
}
